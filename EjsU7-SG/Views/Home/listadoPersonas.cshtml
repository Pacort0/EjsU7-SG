@Html.ActionLink("Página principal", "index")

@model List<EjsU7_SG.Models.Entidades.clsPersona>
@using EjsU7_SG.Models.DAL

<h2>Listado de personas</h2>
<br />
<br />
 
<table>
    <thead>
        <tr>
            <th>Id</th>
            <th>Nombre</th>
            <th>Apellidos</th>
        </tr>
    </thead>
    @foreach (var persona in Model)
    {
        <tr>
            <td>@persona.Id</td>
            <td>@persona.Nombre</td>
            <td>@persona.Apellidos</td>
        </tr>
    }

</table>
